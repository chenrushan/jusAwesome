URxvt.iconFile:             /usr/share/icons/hicolor/scalable/apps/urxvt_icon
URxvt.foreground:           white
URxvt.fading:               10
URxvt.saveLines:            10000
URxvt.internalBorder:       3
URxvt.lineSpace:            0
URxvt.cursorBlink:          false
URxvt.scrollBar:            false
URxvt.urlLauncher:          chromium

urxvt*internalBorder:       0
urxvt*externalBorder:       0

! urxvt clipboard
URxvt.perl-ext-common:      selection-to-clipboard
 
! enable real transparency
URxvt.depth:                32
!gray theme color
!URxvt.background:           rgba:2222/2222/2222/eeee

!green theme color
URxvt.background:           rgba:1111/4444/3333/dddd
!URxvt.background:           rgba:1111/1111/1111/dddd
URxvt.transparent:          false

! fonts
URxvt.allow_bold:           false
URxvt*font:                 xft:Monaco for Powerline:regular:size=10,xft:AR PL UKai CN:regular:size=11
URxvt*imFont:               xft:Monaco for Powerline:regular:size=10,xft:AR PL UKai CN:regular:size=11
URxvt*boldFont:             xft:Monaco for Powerline:bold:size=10,xft:AR PL UKai CN:bold:size=11
URxvt*italicFont:           xft:Monaco for Powerline:italic:size=10,xft:AR PL UKai CN:italic:size=11
! Consolas font is great for bold and italic style
URxvt*boldItalicFont:       xft:Consolas:bold:italic:size=9,xft:AR PL UKai CN:bold:italic:size=11
URxvt*letterSpace:          -1

! dpi control the size of font
Xft.dpi:        96
Xft.antialias:  true
Xft.rgba:       rgb
Xft.hinting:    true
Xft.hintstyle:  hintfull
 
